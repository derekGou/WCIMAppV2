{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/incrementClicks.ts",
        "use": "@vercel/node"
      },
      {
        "src": "api/incrementVisits.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/incrementClicks",
        "methods": ["POST", "GET"],
        "dest": "/api/incrementClicks.ts"
      },
      {
        "src": "/api/incrementVisits",
        "methods": ["POST", "GET"],
        "dest": "/api/incrementVisits.ts"
      },
      {
        "src": "/(.*).js",
        "dest": "/$1.js",
        "headers": {
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }