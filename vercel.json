{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/incrementClicks.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/incrementVisits.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/incrementClicks",
        "dest": "/api/incrementClicks.js"
      },
      {
        "src": "/api/incrementVisits",
        "dest": "/api/incrementVisits.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }